# Database Configuration
# PostgreSQL container settings
POSTGRES_DB=YOUR_POSTGRES_DB
POSTGRES_USER=YOUR_POSTGRES_USER
POSTGRES_PASSWORD=YOUR_POSTGRES_PASSWORD

# Backend database connection settings
DB_HOST=YOUR_DB_HOST
DB_PORT=YOUR_DB_PORT
DB_USER=YOUR_DB_USER
DB_PASSWORD=YOUR_DB_PASSWORD
DB_NAME=YOUR_DB_NAME

# Full database URL (constructed from above variables)
DATABASE_URL=postgresql://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}

# JWT Configuration
# Secret key for JWT token signing
JWT_SECRET=YOUR_JWT_SECRET

# LLM API Configuration
# URL for the LLM API service (use http://llm-api:8000/api/v1/parse-query-only for Docker Compose)
LLM_API_URL=YOUR_LLM_API_URL
# Timeout in milliseconds for LLM API calls
LLM_TIMEOUT=YOUR_LLM_TIMEOUT

# Optional: OpenAI API Key for LLM integration (if needed)
OPENAI_API_KEY=YOUR_OPENAI_API_KEY